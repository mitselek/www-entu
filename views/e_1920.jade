extends layout

block vars

block content
    #cover.container-fluid Kanuti Gildi Saal

        each c, i in childs
            .row.text-center
                a(href=c.get('id'), target='_blank')
                    img.col-xs-6.col-sm-4.col-md-2.col-xs-offset-3.col-sm-offset-4.col-md-offset-5(src=c.get('display.picture.file'), alt=c.get('title.value'), style='margin-bottom:20px')
                h3.col-xs-12= c.get('title.value')
                p.col-xs-12= c.get('url.value')
            .spacer